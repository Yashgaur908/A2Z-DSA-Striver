//  Question link:- https://practice.geeksforgeeks.org/problems/check-if-an-array-is-sorted0701/1

class Solution {
    boolean arraySortedOrNot(int[] arr, int n) {
        for(int i = 0; i < n-1; i++){
            if(arr[i] > arr[i+1])
                return false;
        }
        return true;
    }
}
